{
  "if": {
    "setSkypeUrlOnSkypeDevice": {
      "conditions": {
        "all": [
          {
            "fact": "data",
            "path": ".data.action",
            "operator": "equal",
            "value": "click"
          },
          {
            "fact": "fromDevice",
            "path": ".genisys.room.genisys.devices.skype.uuid",
            "operator": "exists",
            "value": true
          },
          {
            "fact": "fromDevice",
            "path": ".genisys.room.genisys.inSkype",
            "operator": "equal",
            "value": true
          }
        ]
      },
      "event": {
       "retry": true,
        "type": "meshblu",
        "params": {
          "operation": "update",
          "as": "{{device.genisys.customerDevices.customer.uuid}}",
          "uuid": "{{fromDevice.genisys.room.genisys.devices.skype.uuid}}",
          "data": {
            "$set": {
              "desiredState.meeting": null
            }
          }
        }
      }
    },
    "buttonLongClickEndSkype": {
      "conditions": {
        "all": [
          {
            "fact": "data",
            "path": ".data.action",
            "operator": "equal",
            "value": "longClick"
          },
          {
            "fact": "fromDevice",
            "path": ".genisys.room.genisys.currentMeeting",
            "operator": "exists",
            "value": true
          },
          {
            "fact": "fromDevice",
            "path": ".genisys.roomId",
            "operator": "exists",
            "value": true
          }
        ]
      },
      "event": {
       "retry": true,
        "type": "meshblu",
        "params": {
          "operation": "update",
          "as": "{{device.genisys.customerDevices.customer.uuid}}",
          "uuid": "{{fromDevice.roomDevices.skype.uuid}}",
          "data": {
            "$set": {
              "desiredState.meeting" : null
            }
          }
        }
      }
    }
  }
}
