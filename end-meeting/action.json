{
  "rules": {
    "endMeetingInRoom": {
      "conditions": {
        "all": [
          {
            "fact": "data",
            "path": ".metadata.jobType",
            "operator": "equal",
            "value": "end-meeting"
          }
        ]
      },
      "event": {
        "type": "meshblu",
        "params": {
          "operation": "message",
          "message": {
            "devices": ["{{data.data.room.genisys.devices.endoGenisysMeeting.uuid}}"],
            "metadata": {
              "jobType": "EndMeeting"
            },
            "data": {
              "meetingId": "{{data.data.meetingId}}"
            }
          }
        }
      }
    }
  }
}
