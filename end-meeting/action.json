{
  "if": {
    "endMeetingInRoom": {
      "conditions": {
        "all": [
          {
            "fact": "data",
            "path": ".metadata.jobType",
            "operator": "equal",
            "value": "end-meeting"
          }
        ]
      },
      "event": {
       "retry": true,
        "type": "meshblu",
        "params": {
          "operation": "message",
          "as": "{{device.genisys.customerDevices.customer.uuid}}",
          "message": {
            "devices": ["{{device.genisys.customerDevices.meetingGroupEndoMeeting.uuid}}"],
            "metadata": {
              "jobType": "EndMeeting"
            },
            "data": {
              "meetingId": "{{data.data.meetingId}}",
              "roomId": "{{data.data.room.uuid}}"
            }
          }
        }
      }
    }
  }
}
