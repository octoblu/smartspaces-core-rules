{
  "rules": {
    "startMeeting": {
      "conditions": {
        "all": [
          {
            "fact": "data",
            "path": ".metadata.jobType",
            "operator": "equal",
            "value": "start-meeting"
          }
        ]
      },
      "event": {
        "type": "meshblu",
        "params": {
          "operation": "message",
          "message": {
            "devices": ["{{data.room.genisys.devices.endoGenisysMeeting.uuid}}"],
            "metadata": {
              "jobType": "UpsertMeeting"
            },
            "data": {
                "subject": "Instant Meeting",
                "roomId": "{{data.room.uuid}}",
                "roomName": "{{data.room.name}}",
                "location": "{{data.room.genisys.location}}",
                "attendees": []
            }
          }
        }
      }
    }
  }
}
