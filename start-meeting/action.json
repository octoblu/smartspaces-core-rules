{
  "rules": {
    "startMeeting": {
      "conditions": {
        "all": [
          {
            "fact": "data",
            "path": ".genisys.roomId",
            "operator": "exists",
            "value": true
          }
        ]
      },
      "event": {
        "type": "meshblu",
        "params": {
          "operation": "message",
          "message": {
            "devices": ["{{device.genisys.devices.endo-genisys-meeting.uuid}}"],
            "metadata": {
              "jobType": "UpsertMeeting"
            },
            "data": {
                "subject": "Instant Meeting",
                "roomId": "{{data.genisys.roomId}}",
                "attendees": []
            }
          }
        }
      }
    }
  }
}
